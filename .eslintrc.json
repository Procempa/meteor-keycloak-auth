{
    "env": {
        "browser": true,
        "es6": true
    },
		"globals": {
			"Keycloak": true,
      "ClassificacaoServicos": true
		},
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "warn",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [1, "single"],
        "semi": [
            2
            /*"never"*/
        ],
        "no-unused-vars": [
            "warn"
        ],
        "no-console": [
            0
        ]
    }
}
